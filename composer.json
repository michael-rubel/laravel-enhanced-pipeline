{
    "name": "michael-rubel/laravel-enhanced-pipeline",
    "description": "Laravel Pipeline with DB transaction support, events and additional methods.",
    "keywords": [
        "michael-rubel",
        "laravel",
        "laravel-enhanced-pipeline"
    ],
    "homepage": "https://github.com/michael-rubel/laravel-enhanced-pipeline",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Rubel",
            "email": "contact@observer.name",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.11"
    },
    "require-dev": {
        "brianium/paratest": "^6.3",
        "laravel/pint": "^0.2.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.0|^6.0",
        "nunomaduro/larastan": "^1.0|^2.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.5",
        "roave/backward-compatibility-check": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "MichaelRubel\\EnhancedPipeline\\": "src"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MichaelRubel\\EnhancedPipeline\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichaelRubel\\EnhancedPipeline\\EnhancedPipelineServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
