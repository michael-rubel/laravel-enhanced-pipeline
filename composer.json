{
    "name": "michael-rubel/laravel-enhanced-pipeline",
    "description": "Laravel Pipeline with DB transaction support, events and additional methods.",
    "keywords": [
        "michael-rubel",
        "laravel",
        "laravel-enhanced-pipeline"
    ],
    "homepage": "https://github.com/michael-rubel/laravel-enhanced-pipeline",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Rubel",
            "email": "contact@observer.name",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0|^8.0",
        "larastan/larastan": "^2.0",
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^9.5|^10.5"
    },
    "autoload": {
        "psr-4": {
            "MichaelRubel\\EnhancedPipeline\\": "src"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MichaelRubel\\EnhancedPipeline\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichaelRubel\\EnhancedPipeline\\EnhancedPipelineServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
